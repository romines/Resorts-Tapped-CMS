{
  "extends": [
    "eslint:recommended",
    "prettier"
  ],
  "env": {
    "node": true,
    "es6": true
  },
  "plugins": [
    "prettier"
  ],
  "rules": {
    "semi": [
      2,
      "always"
    ],
    "no-console": "off",
    "prettier/prettier": "warn"
  },
  "root": true,
  "parserOptions": {
    "ecmaVersion": 9
  }
}