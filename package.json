{
  "name": "beer",
  "description": "A Vue.js project",
  "version": "1.0.0",
  "author": "Adam Romines <adam.romines@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "deploy": "firebase deploy -P tapped",
    "production-deploy": "cross-env NODE_ENV=production webpack --progress --hide-modules && firebase deploy -P tapped",
    "production-functions": "firebase deploy --only functions -P tapped",
    "staging-deploy": "cross-env NODE_ENV=staging webpack --progress --hide-modules && firebase deploy -P staging",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --host 0.0.0.0 --open --hot --overlay",
    "export": "cd utils && node ./export.js",
    "test": "npm run test:seed && npm run cypress:run",
    "test:build": "cross-env NODE_ENV=testing webpack --progress --hide-modules",
    "test:deploy": "firebase deploy -P testing",
    "test:seed": "cd utils && node ./seedTestData.js",
    "cypress:open": "cross-env CYPRESS_RUN_BINARY=../cypress/build/linux/Cypress/Cypress NODE_ENV=testing cypress open",
    "cypress:run": "cross-env CYPRESS_RUN_BINARY=../cypress/build/linux/Cypress/Cypress NODE_ENV=testing cypress run"
  },
  "dependencies": {
    "@google-cloud/firestore": "3.7.2",
    "awesome-phonenumber": "^2.2.6",
    "axios": "^0.19.1",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "bulma": "^0.7.1",
    "csv-parse": "^4.8.5",
    "deep-equal": "^1.0.1",
    "firebase": "^7.7.0",
    "firebase-admin": "^8.11.0",
    "firebase-functions": "^3.3.0",
    "grpc": "^1.24.9",
    "moment": "^2.22.2",
    "uuid": "^3.4.0",
    "v-click-outside": "^1.0.5",
    "v-viewer": "^1.4.0",
    "vue": "^2.6.10",
    "vue-axios": "^2.1.5",
    "vue-cleave": "^1.2.2",
    "vue-router": "^3.0.7",
    "vue2-editor": "^2.6.6",
    "vuedraggable": "^2.16.0",
    "vuex": "^3.1.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-eslint": "^3.8.0",
    "@vue/eslint-config-prettier": "^4.0.1",
    "babel-core": "^6.26.3",
    "babel-eslint": "^9.0.0-beta.3",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-3": "^6.24.1",
    "cross-env": "^5.2.0",
    "css-loader": "^3.0.0",
    "cypress": "^3.3.1",
    "eslint": "^6.0.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-cypress": "^2.0.1",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-vue": "^5.0.0",
    "file-loader": "^1.1.11",
    "node-sass": "^4.12.0",
    "prettier": "^1.15.3",
    "sass-loader": "^6.0.7",
    "vue-loader": "^15.7.0",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.17.1",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.7.2"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
