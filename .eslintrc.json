{
  "parserOptions": {
    "parser": "babel-eslint",
    "ecmaVersion": 2017,
    "sourceType": "module"
  },
  "extends": ["plugin:vue/essential", "@vue/prettier"],
  "plugins": ["vue", "prettier"],
  "rules": {
    "vue/require-v-for-key": "off",
    "vue/require-default-prop": "off",
    "vue/attributes-order": "off",
    "no-console": "off",
    "for-direction": "off",
    "getter-return": "off",
    "semi": [2, "never"]
  }
}
